{
  "name": "bidzm/mysticquent",
  "license": "MIT",
  "description": "Mysticquent is an Elasticsearch ODM and mapper for Laravel. It is inspired from sleimanx2/plastic and searchkick",
  "keywords": ["elasticsearch","elastic","laravel","eloquent","model","ODM","mapper"],
  "homepage": "https://github.com/abidnurulhakim/mysticquent",
  "authors": [
    {
      "name": "Abid Nurul Hakim",
      "email": "abidnurulhakim@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "elasticsearch/elasticsearch": "~5.3.0",
    "ongr/elasticsearch-dsl": "5.*",
    "illuminate/container": "5.2.*|5.3.*|5.4.*|5.5.*",
    "illuminate/contracts": "5.2.*|5.3.*|5.4.*|5.5.*",
    "illuminate/console": "5.2.*|5.3.*|5.4.*|5.5.*",
    "illuminate/pagination": "5.2.*|5.3.*|5.4.*|5.5.*",
    "illuminate/support": "5.2.*|5.3.*|5.4.*|5.5.*",
    "illuminate/database": "5.2.*|5.3.*|5.4.*|5.5.*"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~5.7",
    "mockery/mockery": "^0.9.4"
  },
  "autoload": {
    "psr-4": {
      "Bidzm\\Mysticquent\\": "src/"
    },
    "files": [
        "src/Helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Bidzm\\Mysticquent\\Tests\\": "tests/"
    }
  }
}
