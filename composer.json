{
  "name": "bidzm/mysticquent",
  "license": "MIT",
  "description": "Mysticquent is an Elasticsearch ODM and mapper for Laravel. It is inspired from sleimanx2/plastic and searchkick",
  "keywords": ["elasticsearch","elastic","laravel","eloquent","model","ODM","mapper"],
  "homepage": "https://github.com/abidnurulhakim/mysticquent",
  "authors": [
    {
      "name": "Abid Nurul Hakim",
      "email": "abidnurulhakim@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "elasticsearch/elasticsearch": "~2.2",
    "ongr/elasticsearch-dsl": "~2.0",
    "illuminate/container": "^5.*",
    "illuminate/contracts": "^5.*",
    "illuminate/console": "^5.*",
    "illuminate/pagination": "^5.*",
    "illuminate/support": "^5.*",
    "illuminate/database": "^5.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "^0.9.4",
    "phpunit/php-code-coverage": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Bidzm\\Mysticquent\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bidzm\\Mysticquent\\Tests\\": "tests/"
    }
  }
}
